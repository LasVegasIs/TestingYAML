{
    "Serilog": {
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "restrictedToMinimumLevel": "Verbose",
            "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] {EventId} {Message:lj} {Properties}<s:{SourceContext}>{NewLine}{Exception}",
            "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
          }
        }
      ]
    }
  }